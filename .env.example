# Database (Required)
DATABASE_URL=postgresql://user:password@host:5432/database_name

# OpenAI API (Required for report generation)
OPENAI_API_KEY=sk-...

# Qdrant Vector Database (Optional)
QDRANT_HOST=localhost
QDRANT_PORT=6333
